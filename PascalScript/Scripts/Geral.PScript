Program Teste;
 
Var
   X, A: REAL;
 
begin
   A := 10;
  
   X := 1 + 3 * 2;       {7}
   X := X + 1;           {8}
   X := X * (1 + 1);     {16}

   A := X * A; 

   if A = 160 then
      if X = 11 then
          X := 1
      else
         X := 2;

   x := SQR(SQR(1.2));
   A := Power(2, x);

   ShowMessage('O valor de x é: ' + ValToStr(x));
end.

